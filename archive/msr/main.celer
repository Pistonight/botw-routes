_project: 
  name: Master Sword Restricted
  authors:
    - iTNTPiston
  version: "3.0"
  description: Get Master Sword without glitches that allow you to get it early
  url: ""

CheckPepper:
  At least .v(Pepper) half-heart food left

_config:
  split-format:
    Shrine: "[{{%02 Shrine}}] {{_}}"


_route:
